@model BooksViewModel


@foreach (var b in Model.Categories.Select(x => x.Category).Distinct().OrderBy(x => x))
            {
                <a class="dropdown-item" asp-controller="Home" asp-action="Index" asp-route-bookCategory="@b" asp-rout-pageNum="1">@b</a>
            }